{
  "expo": {
    "name": "camera-scanner",
    "slug": "camera-scanner",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "react-native-document-scanner-plugin",
        {
          "cameraPermission": "We need camera access, so you can scan documents"
        }
      ]
    ],
    "android": {
      "package": "com.anonymous.camerascanner"
    },
    "ios": {
      "bundleIdentifier": "com.anonymous.camerascanner"
    }
  }
}
